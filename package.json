{
  "name": "twitch-sentry",
  "version": "1.4.4",
  "description": "Configurable Twitch bot, running Node.js using RxJS, TypeScript, lowdb and tmi.",
  "main": "dist/app/app.js",
  "scripts": {
    "start": "node dist/app/app.js",
    "watch": "nodemon 'src/app/app.ts' --ignore 'db.json' --ignore 'dist'",
    "build": "tsc"
  },
  "author": "spnq",
  "license": "MIT",
  "dependencies": {
    "lowdb": "^1.0.0",
    "rxjs": "^6.6.6",
    "tmi.js": "^1.7.5"
  },
  "devDependencies": {
    "@types/lowdb": "^1.0.9",
    "@types/node": "^12.20.5",
    "@types/tmi.js": "^1.7.1",
    "nodemon": "^1.19.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9",
    "lodash": ">=4.17.19"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spnq/twitch-sentry.git"
  },
  "keywords": [
    "twitch",
    "bot",
    "twitch-bot",
    "nodejs",
    "javascript",
    "rxjs",
    "typescript"
  ]
}
